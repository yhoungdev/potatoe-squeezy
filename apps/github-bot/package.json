    "ts-node": "^10.9.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^24.0.1",
    "nock": "^14.0.0-beta.5",
    "smee-client": "^4.2.1",
    "typescript": "^5.3.3",
    "vitest": "^3.2.3"
  },
  "engines": {
    "node": ">= 18"
